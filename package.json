{
  "name": "reception",
  "version": "1.0.0",
  "description": "BuddyPress plugin to customize user front using WordPress Blocks.",
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.8"
  },
  "scripts": {
    "start": "parcel watch src/*/*.js --out-dir js --no-source-maps",
    "dev": "parcel build src/*/*.js --out-dir js --no-source-maps --no-minify",
    "build": "parcel build src/*/*.js --out-dir js",
    "wpcs": "./vendor/bin/phpcs class-reception.php inc/* --extensions=php --standard=WordPress",
    "makepot": "wp i18n make-pot . languages/reception.pot --exclude=\".git,.cache,css,images,languages,node_modules,src,vendor\" --headers='{\"Project-Id-Version\": \"RÃ©ception\", \"Report-Msgid-Bugs-To\": \"https://github.com/imath/reception/issues\", \"Last-Translator\": \"imath <contact@imathi.eu>\", \"Language-Team\": \"FRENCH <contact@imathi.eu>\"}'",
    "makejson": "wp i18n make-json languages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imath/reception.git"
  },
  "keywords": [
    "buddypress",
    "members",
    "front"
  ],
  "author": "imath",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/imath/reception/issues"
  },
  "homepage": "https://github.com/imath/reception#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@wordpress/babel-preset-default": "^4.12.1",
    "parcel-bundler": "^1.12.4"
  }
}
